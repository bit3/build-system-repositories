{
	"name": "contao-community-alliance/build-system-repositories",
	"description": "Multiple repositories manager for CCABS.",
	"license": "MIT",
	"minimum-stability": "dev",
	"require": {
		"contao-community-alliance/build-system-repository-git": "~1.0",
		"symfony/console": "~2.3",
		"symfony/filesystem": "~2.3",
		"symfony/yaml": "~2.3",
		"guzzle/guzzle": "~3.0"
	},
	"require-dev": {
		"monolog/monolog": "~1",
		"phpunit/phpunit": "3.7.*",
		"symfony/filesystem": "~2.3"
	},
	"autoload": {
		"psr-4": {
			"ContaoCommunityAlliance\\BuildSystem\\Repositories\\": "src",
			"ContaoCommunityAlliance\\BuildSystem\\Repositories\\Test\\": "test"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	}
}
